        -scale_angle "0" \
        -scale_font_att {normal} \
        -scale_font_color "#000000" \
        -scale_font_family "arial" \
        -scale_font_size "10" \
        -scale_format "preferred" \
        -scale_padding "0" \
        -scale_precision "6" \
