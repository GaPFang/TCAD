# Plot configuration module
#===============================================================================================
proc restore_plot { } {

    # Applying Plot properties
    #-------------------------------------------------------------------------------------------
    set_plot_prop \
        -title {Band Diagram} \
        -show_title  \
        -title_font_family "arial" \
        -title_font_color "#000000" \
        -title_font_size "10" \
        -title_font_att {normal} \
        -color_fg "#000000" \
        -color_bg "#ffffff" \
        -frame_width "1" \
        -show_axes  \
        -show_axes_title  \
