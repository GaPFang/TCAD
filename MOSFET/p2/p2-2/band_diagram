    #-------------------------------------------------------------------------------------------
return [array get datasetList]}
#===============================================================================================



# Curves initialization module
#===============================================================================================
proc init_plot { data } {
    array set datasetList $data

    # Creating a new plot 1D
    #-------------------------------------------------------------------------------------------
    set plotName [ create_plot -1d ]
    select_plots $plotName
    #-------------------------------------------------------------------------------------------

    foreach index [array names datasetList] {
        # Creating a dataset per each curve
        #---------------------------------------------------------------------------------------
        set datasetName "Dataset_$index"
        create_variable  -dataset $datasetName  -name X  -values [lindex $datasetList($index) 0]
        create_variable  -dataset $datasetName  -name Y  -values [lindex $datasetList($index) 1]
        #---------------------------------------------------------------------------------------

        # Creating a curve
        #---------------------------------------------------------------------------------------
        create_curve  -name $index  -dataset $datasetName  -axisX X  -axisY Y
        #---------------------------------------------------------------------------------------
    }
}
#===============================================================================================



# Creating Plot and Curves with restored data
#-----------------------------------------------------------------------------------------------
init_plot [ init_data ]
#-----------------------------------------------------------------------------------------------

# Restoring Plot configuration
#-----------------------------------------------------------------------------------------------
restore_plot
#-----------------------------------------------------------------------------------------------

# Restoring Curves configuration
#-----------------------------------------------------------------------------------------------
restore_curves
#-----------------------------------------------------------------------------------------------

# Restoring Drawings
#-----------------------------------------------------------------------------------------------
restore_drawings
#-----------------------------------------------------------------------------------------------

