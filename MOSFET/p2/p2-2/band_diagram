        -spacing "0.11" \
        -auto_spacing  \
        -ticks_position "in" \
        -show_ticks  \
        -title {X} \
        -title_font_att {normal} \
        -title_font_color "#000000" \
        -title_font_family "arial" \
        -title_font_size "10" \
        -show_title  \
        -type "linear"
