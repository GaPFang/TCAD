# Curves configuration module
#===============================================================================================
proc restore_curves { } {
    # Restoring Curve properties
    #-------------------------------------------------------------------------------------------
    # Curve_1
    set curveName "Curve_1"
    set_curve_prop $curveName \
        -label {ConductionBandEnergy(C2(MOSFET_Vds0V_des))} \
